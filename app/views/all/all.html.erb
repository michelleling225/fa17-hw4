<div>
<h1>Your username</h1>
<% @users.each do |user| %>
   <%= user.username %>
<%end %>
<h1> Your age </h1>
<% @users.each do |user| %>
   <%= user.age %>
<%end %>
<h3>Your email</h3>
  <% @users.each do |user| %>
   <%= user.email %>
<%end %>
<h1> Your cats' name </h1>
  <% @cats.each do |cat| %>
   <%= cat.name %>
<%end %>
<h2> Your todos </h2>
  <% @todos.each do |todo| %>
   <%= todo.tasks %>
   <h3> Finished? </h3>
   <%= todo.finished %>
<%end %>

</div>